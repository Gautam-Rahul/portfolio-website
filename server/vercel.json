{
  "version": 2,
  "builds": [
    {
      "src": "vercel-entry.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/vercel-entry.js"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel-entry.js"
    }
  ]
} 